;;; Compares

.import ../src/ladybug

.postfix PSEI, DSZI, DECI, INCI, WSIZE, LDI, STI, CMP, SB, CB, B?  (semi-merged-postfix)
.postfix MASKL, MASKR, BITSUM, TST, SL, SR  (semi-merged-postfix)
.postfix #LIT (semi-merged-integer-literal)

LBL "TCMP"
WSIZE 8
SF 02
MASKL 8
#LIT 0x7f
CMP Y
LE?
SF 99
GT?
GTO 09
SF 99
LBL 09
GE?
GTO 09
SF 99
LBL 09

#LIT 0x30
#LIT 0xaa
CMP Y
LT?
GTO 09
SF 99
LBL 09

GE?
SF 99

CF 02
ENTER
CMP Y
GT?
SF 99

WSIZE 64
XEQ 10
WSIZE 57
XEQ 10
WSIZE 56
XEQ 10
WSIZE 16

LBL 10

CF 02          ; unsigned compare
XEQ 11

XEQ 03
XEQ 12

XEQ 02
XEQ 13

XEQ 04
XEQ 12

SF 02    ; signed compare
XEQ 11

XEQ 03
XEQ 12

XEQ 02
XEQ 12

XEQ 04
GTO 13


LBL 01          ; 4 5
#LIT  4
#LIT  5
RTN

LBL 03         ; -4 -5
XEQ 01
X<>YI
NEG
X<>YI
NEG
RTN

LBL 02          ; 4 -5
XEQ 01
NEG
RTN

LBL 04          ; -4 5
XEQ 01
X<>YI
NEG
X<>YI
RTN


LBL 12
CMP Y
GE?
SF 99
GT?
SF 99
LT?
GTO 09
SF 99
LBL 09
LE?
RTN
SF 99

LBL 13
CMP Y
GE?
GTO 09
SF 99
LBL 09
GT?
GTO 09
SF 99
LBL 09
LT?
SF 99
LE?
SF 99
RTN

; Positive numbers behave same in both signed and unsigned,
; so we use this common code for them.
LBL 11
XEQ 01
CMP Y
GE?
GTO 09
SF 99
LBL 09
GT?
GTO 09
SF 99
LBL 09
LT?
SF 99
LE?
SF 99

DECI X        ; make both 4
CMP Y
GE?
GTO 09
SF 99
LBL 09
LE?
GTO 09
SF 99
LBL 09

FC? 00       ; unsigned equal (and plus) using zero/sign flags
SF 99
FS? 01
SF 99

END
